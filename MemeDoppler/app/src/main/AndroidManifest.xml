<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chad.memedoppler">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/mostly_sunny"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DisplayMessageActivity" />
        <activity android:name=".ExpandedMeme" />
        <activity android:name=".ExpandedForecastOne" />
        <activity android:name=".ExpandedForecastTwo" />
        <activity android:name=".ExpandedForecastThree" />
        <activity android:name=".ExpandedForecastFour" />
        <activity android:name=".ExpandedForecastFive" />
        <activity android:name=".ExpandedForecastSix" />
        <activity android:name=".NotDank"></activity>
    </application>

</manifest>